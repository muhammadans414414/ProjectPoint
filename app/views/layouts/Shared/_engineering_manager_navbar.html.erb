<div class="flex  p-3 px-5 bg-blue-50">
  <div class="mb-2 sm:mb-0 flex ">
    <div class="h-10 mr-3">
      <%=image_tag("logo.png",class:"h-16")%>
    </div>
    <div  class=" pt-3">
      <%= link_to "Visibility", engineering_managers_users_path, class:"text-2xl text-purple-600"%>
      <%= link_to "Point", engineering_managers_users_path, class:"text-2xl "%>
    </div>
  </div>
  <div class="mx-8 flex justify-between items-center w-5/6">
    <div>
          <ul class="flex flex-col p-4 mt-4 rounded-lg border border-gray-100 md:flex-row text-white md:space-x-8 md:mt-0 md:text-sm md:font-medium md:border-0 dark:bg-gray-800 md:dark:bg-gray-900 dark:border-gray-700">
        <% if user_signed_in? %>
          <li>
            <%= link_to 'See Profile',  "/admins/profile_details", class:"text-base text-black" %>
          </li>
         <li>
          <%= link_to 'Projects',  engineering_managers_projects_path, class: "text-base text-black" %>
        </li>
        <li>
          <%= link_to 'Developers',  engineering_managers_developers_path, class: "text-base text-black" %>
        </li>
           <li>
         <%= link_to 'Skills',  engineering_managers_skills_path, class:"text-base text-black"%>
        </li>
        <% else %>
          <li>
            <%= link_to 'Sign In', new_user_session_path %>
          </li>
          <li>
            <%= link_to 'Register', new_user_registration_path%>
          </li>
        <% end %>
      </ul>
    </div>
    <div class="pt-2">
      <%= link_to "Sign out", destroy_user_session_path, data: { turbo_method: :delete},class:"text-white bg-red-700 hover:bg-red-400 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center mr-3 md:mr-0 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800"%>
    </div>
  </div>
</div>
