<h1> Assign Project </h1>
<%= form_with model:[:engineering_managers,@user_project], data:{turbo:false} do |f| %>
  <div class="field">
    <%= f.label :"Project" %>
    <%= f.collection_select :project_id, Project.all, :id, :full_name, prompt:true,required:true%>
  </div>
  <div>
    <%= f.submit "Assign Project" %>
  </div>
<%end%>
